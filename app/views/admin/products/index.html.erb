<h2>商品一覧</h2>
<table class="table">
 <thead>
  <tr>
   <th>商品ID</th>
   <th>商品名</th>
   <th>税抜価格</th>
   <th>ジャンル</th>
  </tr>
 </thead>
 <tbody>
  <% @products.each do |product| %>
  <tr>
   <td><%= product.id %></td>
   <td><%= product.name %></td>
   <td><%= product.non_taxed_price %></td>
   <td><%= product.genre.name %></td>
  </tr>
  <% end %>
 </tbody>
</table>
