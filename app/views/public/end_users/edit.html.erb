<div class="container">
 <div class="row">
  <div class="col-md-8 mt-3">
   <h3><span class="bg-light">会員情報編集</span></h3>
    <%= form_with model: @end_user do |f| %>
     <table class="table table-borderless">
     
        <tbody>
          <tr>
            <td>氏名</td>
            <td><%=  f.text_field :last_name %> <%= f.text_field :first_name %></td>
          </tr>
          <tr>
            <td>フリガナ</td>
            <td><%=  f.text_field :last_name_kana %> <%=  f.text_field :first_name_kana %></td>
          </tr>
          <tr>
            <td>郵便番号</td>
            <td><%=  f.text_field :postcode %></td>
          </tr>
          <tr>
            <td>住所</td>
            <td><%=  f.text_field :address %></td>
          </tr>
          <tr>
            <td>電話番号</td>
            <td><%=  f.text_field :phone_number %></td>
          </tr>
          <tr>
            <td>メールアドレス</td>
            <td><%=  f.text_field :email %></td>
          </tr>
        </tbody>
    </table> 
        <div class="text-center">
          <%= f.submit '変更内容を保存', class: "btn btn-success mr-3" %>
          <%= link_to "退会する", end_users_unsubcribe_path, class: "btn btn-danger" %>
        </div>  
        <% end %>
        
       
      
    </div>
  </div>
</div>